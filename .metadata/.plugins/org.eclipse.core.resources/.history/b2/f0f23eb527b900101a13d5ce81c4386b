package com.Functions;

public class D06MethodOverloading {

	public static void calculateAddition1(int n1, int n2) {
		int sum = n1 + n2;
		System.out.println("Addition: " + sum);
	}
	public static void calculateAddition2(int n1, int n2, int n3) {
		int sum = n1 + n2 + n3;
		System.out.println("Addition: " + sum);
	}
	
	public static void client() {
		calculateAddition1(10, 20);
		calculateAddition2(10, 20, 30);
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		client();
	}

}
