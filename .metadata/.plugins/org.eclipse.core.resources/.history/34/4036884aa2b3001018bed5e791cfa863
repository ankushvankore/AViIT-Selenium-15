package com.BasicPrograms;

public class D02Variables {
	public static void main(String[]args) {
		int no = 89;
		double pi = 3.142;
		char grade = 'A';
		String application = "Eclipse";
		
		System.out.println("Marks obtained: " + no);
		System.out.println("Value of Pi   : " + pi);
		System.out.println("Grade         : " + grade);
		System.out.println("Editor using  : " + application);
	}
}
